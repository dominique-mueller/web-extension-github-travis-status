{
    "name": "GitHub & Travis CI - Extended Status",
    "version": "1.0.0",
    "description": "TODO",
    "manifest_version": 2,
    "permissions": [
        "https://github.com/*",
        "tabs",
        "webNavigation"
    ],
    "background": {
        "scripts": [
            "src/background.js"
        ],
        "persistent": false
    },
    "content_scripts": [
        {
            "matches": [
                "https://github.com/*"
            ],
            "js": [
                "src/content_script.js",
                "src/vendor.js"
            ],
            "css": [
                "src/content_script.css"
            ]
        }
    ]
}
